@using System.Drawing
@model Web.Models.ListaLogActividadDto
@{
    Layout = null;
}
      <h2><strong>@Model.Titulo</strong></h2>
<table class="table">
      <thead>
        <tr>
          <th>#</th>
          <th>NumeroDeTarjeta</th>
          <th>Fecha</th>
        </tr>
      </thead>
      <tbody>
        @foreach (var item in Model.LogActividades)
        {
            <tr>
              <th scope="row">@item.Id</th>
              <td>@item.NumeroDeTarjeta</td>
              <td>@item.Fecha</td>
            </tr>
        }
      </tbody>
    </table>


